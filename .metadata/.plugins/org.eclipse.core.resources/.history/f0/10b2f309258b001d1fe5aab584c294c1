package com.bit;


class Lec06 extends Thread{
	@Override
	public void run() {
		// TODO Auto-generated method stub
		for (int i = 0; i < 50; i++) {
			System.out.println(i+1+"번 수행...");
		}
	}
}

public class Ex06 {
	public static void main(String[] args) {
		Ex06 me = new Ex06();
		try {
			me.wait();
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		me.notify();
	}
}
