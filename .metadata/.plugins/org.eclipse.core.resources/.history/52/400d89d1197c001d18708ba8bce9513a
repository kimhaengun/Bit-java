package com.bit;

public class Ex04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "abcdfg";
		System.out.println(str.charAt(0)); //String 문자열 줄 원하는 인덱스의 값을 가져옴
		System.out.println(str.charAt(str.length()-1));
		System.out.println(str.contains("cdf")); // 해당 문자열이 포함되어 있는지
		System.out.println(str.contains("CDF"));
		//문자열 검사 --> startsWith, endsWith
		System.out.println(str.startsWith("b"));
		System.out.println(str.endsWith("g"));
	}

}
