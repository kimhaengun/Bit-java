package com.bit;

class Lec05 extends Thread{
	int su1,su2;
	int sum;
	public Lec05(int su1, int su2) {
		this.su1 = su1;
		this.su2 = su2;
	}
	public void hap(int a) {
		sum+=a;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		for (int i = su1; i <= su2; i++) {
			hap(i);
		}
		System.out.println(su1+"~"+su2+"="+sum);
	}
}
public class Ex05 {
	public static void main(String[] args) {
		Lec05 lec1 = new Lec05(1, 50);
		Lec05 lec2 = new Lec05(51, 100);
		lec1.run();
		lec2.run();
	}
}
