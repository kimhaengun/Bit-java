package com.bit;

class Lec04 extends Thread{
	boolean boo;
	public Lec04(boolean boo) {
		this.boo = boo;
	}
	@Override
	public void run() {
		for (int i = 0; i < 100; i++) {
			if(boo)yield();
			System.out.println(getName()+" working ...");
		}
	}
	
}
public class Ex04 {

	public static void main(String[] args) {
		Lec04 ex1 = new Lec04(false);
		Lec04 ex2 = new Lec04(true);
		ex1.start();
		ex2.start();
	}
}
